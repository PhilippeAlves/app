<div class="container">
 
    <nav class="navbar navbar-default">
      <div class="container-fluid">
        <div class="navbar-header">
          <a class="navbar-brand" href="#"><%= t('model.user_frequency') %></a>
          <ul class="nav navbar-nav brand">
            <li><%= link_to t('action.new'), new_user_frequency_path %></li>
          </ul>
        </div>
      </div>
    </nav>

    <table class="table table-bordered table-hover">
      <thead>
        <tr>
          <th><%= t('head.date') %></th>
          <th><%= t('head.user') %></th>
          <th><%= t('head.type_frequency') %></th>
        </tr>
      </thead>

      <tbody>
        <% @user_frequencies.each do |user_frequency| %>
          <tr>
            <td><%= format_date_br user_frequency.date %> Ã s <%= format_time user_frequency.date %></td>
            <td><%= user_frequency.user.name %></td>
            <td><%= user_frequency.type_frequency %></td>
           
          </tr>
        <% end %>
      </tbody>
    </table>

</div>
