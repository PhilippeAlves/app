<p id="notice"><%= notice %></p>

<h1>Listing User Frequencies</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>User</th>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_frequencies.each do |user_frequency| %>
      <tr>
        <td><%= user_frequency.date %></td>
        <td><%= user_frequency.user %></td>
        <td><%= user_frequency.type %></td>
        <td><%= link_to 'Show', user_frequency %></td>
        <td><%= link_to 'Edit', edit_user_frequency_path(user_frequency) %></td>
        <td><%= link_to 'Destroy', user_frequency, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User frequency', new_user_frequency_path %>
